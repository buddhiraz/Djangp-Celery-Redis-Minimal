{
    "family": "django-task",
    "containerDefinitions": [
      {
        "name": "django",
        "image": "826875819774.dkr.ecr.us-east-1.amazonaws.com/django-celery-redis-backend:latest",
        "memory": 512,
        "cpu": 256,
        "essential": true,
        "portMappings": [
          {
            "containerPort": 8000,
            "hostPort": 8000
          }
        ],
        "environment": [
          {
            "name": "CELERY_BROKER_URL",
            "value": "redis://redis:6379/0"
          },
          {
            "name": "CELERY_RESULT_BACKEND",
            "value": "redis://redis:6379/0"
          }
        ]
      },
      {
        "name": "redis",
        "image": "redis:6.2",
        "memory": 512,
        "cpu": 256,
        "essential": true,
        "portMappings": [
          {
            "containerPort": 6379,
            "hostPort": 6379
          }
        ]
      }
    ]
  }
  